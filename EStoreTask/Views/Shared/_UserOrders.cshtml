@model List<Order> ;



<div class="container-fluid">
  <div class=" cart py-5" >
    <table class="table table-responsive  table-striped table-bordered table-hover text-center " >

      <thead class="table-dark">
        <tr>
          <th>Order ID</th>
          <th>Total Price</th>
        </tr>
      </thead>

      <tbody>
           
        @foreach(var item in Model){
         <tr >
          <td>@item.OrderId</td>
          <td>@item.TotalPrice</td>
        </tr>
        }
         

      </tbody>

      </table>
      @if(Model is null)
          {
                <div class="bg-danger mx-auto text-center fs-2 py-3 px-3 text-white">
                No Orders Found
            </div>

           }
      <hr class="border border-primary border-3 opacity-50">
  </div>
</div>

